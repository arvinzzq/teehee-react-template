{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "version": "{{ version }}",
  "author": "{{ author }}",
  "license": "{{ license }}",
  "scripts": {
    "dev": "node_modules/.bin/webpack-dev-server -d --config config/webpack.dev.config.js",
    "build": "node_modules/.bin/webpack -p --hide-modules --config config/webpack.prod.config.js",
    "lib": "npm run build && cp src/*.css lib",
    "release": "npm run lib && npm publish"
  },
  "keywords": [
  {{#each keywords}}
    "{{this}}"{{#if @last}}{{else}},{{/if}}
  {{/each}}
  ],
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "react-dom": "^16.3.0",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1",
    "react": "^16.3.0"
  }
}
